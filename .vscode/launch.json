  "version": "0.2.0",
  "configurations": [

    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Chat",
      "program": "${workspaceFolder}/target/debug/aita",
      "args": [
        "chat",
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Configuration Menu",
      "program": "${workspaceFolder}/target/debug/aita",
      "args": [
        "config",
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Profile Setup",
      "program": "${workspaceFolder}/target/debug/aita",
      "cwd": "${workspaceFolder}",
      "sourceLanguages": ["rust"],
      "preLaunchTask": "cargo-build"
    }
  ]
}